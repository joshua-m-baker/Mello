<h1> New List </h2>
<%= form_for @changeset, Routes.note_path(@conn, :create), fn f-> %>

    <%= if @changeset.action do %>
        <div class="alert alert-danger">
            <p>Oops, something went wrong! Please check the errors below. </p>
        </div>
    <% end %>

    <div>
        Name: <%= text_input f, :name%>
        <%= error_tag f, :name %>
    </div>
    
    <%= submit "Submit" %>
<% end %>